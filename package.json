{
  "name": "fileupserve",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "npm run build && concurrently \"tsc -w\" \"npm run start\"",
    "cypress:open": "cypress open",
    "cypress:run": "npm run build && concurrently -s first -k \"node ./lib/bin/www.js\" \"cypress run --record\"",
    "test": "npm run cypress:run"
  },
